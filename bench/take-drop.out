drop :: Int -> [A] -> [A]
-- testing 60 combinations of argument values
-- pruning with 0/0 rules
-- looking through 2 candidates of size 1
-- looking through 3 candidates of size 2
-- looking through 3 candidates of size 3
-- looking through 7 candidates of size 4
-- looking through 52 candidates of size 5
-- looking through 111 candidates of size 6
-- looking through 288 candidates of size 7
-- looking through 691 candidates of size 8
drop 0 []  =  []
drop 0 (x:xs)  =  x:xs
drop x []  =  []
drop x (y:xs)  =  drop (dec x) xs

take :: Int -> [A] -> [A]
-- testing 60 combinations of argument values
-- pruning with 14/18 rules
-- looking through 2 candidates of size 1
-- looking through 5 candidates of size 2
-- looking through 17 candidates of size 3
-- looking through 44 candidates of size 4
-- looking through 184 candidates of size 5
-- looking through 663 candidates of size 6
-- looking through 2236 candidates of size 7
-- looking through 7702 candidates of size 8
take 0 []  =  []
take 0 (x:xs)  =  []
take x []  =  []
take x (y:xs)  =  y:take (dec x) xs

